# Used for general Google services, if needed by specific tools.
# For CrewAI script generation with Gemini, GEMINI_API_KEY is used.
GOOGLE_API_KEY="YOUR_GOOGLE_API_KEY_HERE" # Example: Used by tools like GoogleSearchTool

# Used for Gemini LLM
GEMINI_API_KEY="YOUR_GEMINI_API_KEY_HERE" # Required for Gemini models

# Placeholder for OpenAI models (e.g., ChatGPT) - Not currently implemented for script generation
OPENAI_API_KEY="YOUR_OPENAI_API_KEY_HERE_PLACEHOLDER"

# Used for DeepSeek LLM
DEEPSEEK_API_KEY="YOUR_DEEPSEEK_API_KEY_HERE" # Required for DeepSeek models

# Optional: Base URL for your local Ollama API.
# If you have Ollama running and want to use its models, uncomment and set this.
# Default is usually http://localhost:11434 if Ollama runs on the same machine.
OLLAMA_API_BASE_URL="http://localhost:11434" # Optional: Set if using Ollama models

# Optional: API key for Serper.dev for Google Search Tool via serper
SERPER_API_KEY="YOUR_SERPER_API_KEY_HERE"

# Optional: API key for OpenRouter.ai
OPENROUTER_API_KEY="YOUR_OPENROUTER_API_KEY_HERE"

# Optional: API key for Anthropic models
ANTHROPIC_API_KEY="YOUR_ANTHROPIC_API_KEY_HERE"
